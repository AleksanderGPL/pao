{
  "imports": {
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.7.0",
    "@types/pg": "npm:@types/pg@^8.15.4",
    "drizzle-kit": "npm:drizzle-kit@^0.31.4",
    "drizzle-orm": "npm:drizzle-orm@^0.44.2",
    "hono": "npm:hono@4.8.0",
    "ioredis": "npm:ioredis@^5.6.1",
    "pg": "npm:pg@^8.16.3",
    "@/": "./",
    "zod": "npm:zod@^3.25.67"
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env main.ts",
    "dev": "deno run --allow-net --allow-env --env-file=.env --watch main.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "nodeModulesDir": "auto"
}
